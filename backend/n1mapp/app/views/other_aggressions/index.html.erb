<p id="notice"><%= notice %></p>

<h1>Other Aggressions</h1>

<table>
  <thead>
    <tr>
      <th>Aggression detail</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @other_aggressions.each do |other_aggression| %>
      <tr>
        <td><%= other_aggression.aggression_detail %></td>
        <td><%= other_aggression.name %></td>
        <td><%= other_aggression.description %></td>
        <td><%= link_to 'Show', other_aggression %></td>
        <td><%= link_to 'Edit', edit_other_aggression_path(other_aggression) %></td>
        <td><%= link_to 'Destroy', other_aggression, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Other Aggression', new_other_aggression_path %>
